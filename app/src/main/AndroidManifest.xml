<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.check.main">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logogia"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logogia"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SplashActivity"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.result.ListThreeMonthActivity"></activity>
        <activity android:name=".activity.result.ListSixMonthActivity" />
        <activity android:name=".activity.result.ListYearlyActivity" />
        <activity android:name=".activity.result.ListMonthlyActivity" />
        <activity android:name=".activity.result.ListDailyActivity" />
        <activity android:name=".activity.result.ListTransisiActivity" />
        <activity android:name=".activity.result.ResultActivity" />
        <activity android:name=".activity.result.DetailYearlyActivity" />
        <activity android:name=".activity.result.DetailSixMonthActivity" />
        <activity android:name=".activity.result.DetailThreeMonthActivity" />
        <activity android:name=".activity.result.DetailMonthlyActivity" />
        <activity android:name=".activity.result.DetailDailyActivity" />
        <activity android:name=".activity.result.DetailTransisiActivity" />
        <activity android:name=".activity.maintenance.YearlyCheckActivity" />
        <activity android:name=".activity.maintenance.SixMonthCheckActivity" />
        <activity android:name=".activity.maintenance.ThreeMonthCheckActivity" />
        <activity android:name=".activity.schedule.ScheduleActivity" />
        <activity android:name=".activity.maintenance.MonthlyCheckActivity" />
        <activity android:name=".activity.maintenance.DailyCheckActivity" />
        <activity android:name=".activity.maintenance.SessionActivity" />
        <activity android:name=".activity.auth.LoginActivity" />
        <activity android:name=".activity.WelcomeActivity" />
        <activity android:name=".activity.maintenance.ListSessionActivity" />
        <activity android:name=".activity.maintenance.MaintenanceActivity" />
    </application>

</manifest>